@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="zh">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <title>上海三凯建设管理咨询有限公司--@ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="description" content="上海三凯建设管理咨询有限公司" />
    <meta name="viewport" content="width=device-width" />

    <!--modernizr--->
    <script src="~/Scripts/modernizr-2.6.2.js"></script>

    <!--样式部分开始-->
    <!--基本样式-->
    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/assets/css/bootstrap-responsive.min.css" rel="stylesheet" />
    <link href="~/assets/css/font-awesome.min.css" rel="stylesheet" />
    <!--[if IE 7]>
        <link rel="stylesheet" href="~/assets/css/font-awesome-ie7.min.css" />
    <![endif]-->

    <link href="~/assets/css/jquery-ui-1.10.3.full.min.css" rel="stylesheet" />
    <link href="~/Content/validationEngine.jquery.css" rel="stylesheet" />
    <!--各类插件的样式-->
    @RenderSection("pluginCss", required: false)

    <!--字体样式-->
    <link rel="stylesheet" href="~/assets/css/ace-fonts.css" />

    <!--ace样式-->
    <link rel="stylesheet" href="~/assets/css/ace.min.css" />
    <link rel="stylesheet" href="~/assets/css/ace-responsive.min.css" />
    <link rel="stylesheet" href="~/assets/css/ace-skins.min.css" />

    <!--日期选择栏样式-->
    <link href="~/Scripts/My97DatePicker/skin/default/datepicker.css" rel="stylesheet" />

    <!--[if lte IE 8]>
        <link rel="stylesheet" href="~/assets/css/ace-ie.min.css" />
    <![endif]-->
    <!--子界面自定义样式-->
    @RenderSection("customCss", required: false)
    <!--样式部分结束-->
    <!--脚本开始-->
    <!--basic scripts-->
    @*<script src="~/assets/js/jquery-1.10.2.min.js"></script>*@
    <script src="~/Scripts/jquery-1.8.2.min.js"></script>
    <script src="~/assets/js/jquery-ui-1.10.3.full.min.js"></script>
    <script src="~/Scripts/jquery.tmpl.min.js"></script>
    <script src="~/Scripts/jquery.validationEngine-zh_CN.js"></script>
 
    <!--ace settings handler-->
    <script src="~/assets/js/ace-extra.min.js"></script>

    <script src="~/Scripts/knockout-2.2.0.js"></script> 

    <style type="text/css">
        td:nth-child(odd)
        {
            text-align:right;
        }
    </style>
</head>
<body>
    <div style="position: relative;">

        <!-- 顶部按钮开始 -->
        <div class="navbar" id="navbar">
            <script type="text/javascript">
                try { ace.settings.check('navbar', 'fixed') } catch (e) { }
            </script>
            <div class="navbar-inner">
                <script type="text/javascript">
                    //判断是否为IE6-8
                    if (!Modernizr.borderradius) {
                        document.write('<div class="alert alert-warning container-fluid">');
                        document.write('<button type="button" class="close" data-dismiss="alert"><i class="icon-remove"></i></button>');
                        document.write('<img src="@Url.Content("~/Images/ie-notice.png")" />');
                        document.write('<strong>为了更好的使用本系统，请将浏览器升级到IE9+。如果您正在使用IE9+，请将浏览器的兼容模式关闭。</strong>');
                        document.write('</div>');
                    }
                    //if (!$.support.leadingWhitespace) {

                    //}
                </script>

                <div class="container-fluid">
                    <a href="#" class="brand">
                        <small>
                            <img src="~/Images/logo.png" style="height: 22px" />
                            @*<i class="icon-leaf"></i>*@
                            信息化平台
                        </small>
                    </a>
                    <!--/.brand-->
                    <ul class="nav ace-nav pull-right">

 
                        <li class="light-blue">
                            <a data-toggle="dropdown" href="#" class="dropdown-toggle">
                                <span class="user-info">
                                    <small>欢迎,</small>
                                    
                                </span>

                                <i class="icon-caret-down"></i>
                            </a>

                            <ul class="user-menu pull-right dropdown-menu dropdown-yellow dropdown-caret dropdown-closer">
                                
                                <li>
                                    <a href="@Url.Action("Contact", "Home")">
                                        <i class="icon-user"></i>
                                        个人信息
                                    </a>
                                </li>
                                <li class="divider"></li>

                                <li>
                                    <a href="@Url.Action("LogOut", "Account")">
                                        <i class="icon-off"></i>
                                        注销
                                    </a>

                                </li>
                            </ul>
                        </li>
                    </ul>
                    <!--/.ace-nav-->
                </div>
                <!--/.container-fluid-->
            </div>
            <!--/.navbar-inner-->
        </div>
        <!-- 顶部按钮结束 -->
        <!-- 主体部分 -->
        <div class="main-container container-fluid">
            <a class="menu-toggler" id="menu-toggler" href="#">
                <span class="menu-text"></span>
            </a>
            <div class="sidebar" id="sidebar">
                <script type="text/javascript">
                    try { ace.settings.check('sidebar', 'fixed') } catch (e) { }
                </script>
                <!-- 顶部右侧按钮 -->
                <div class="sidebar-shortcuts" id="sidebar-shortcuts">
                    <div class="sidebar-shortcuts-large" id="sidebar-shortcuts-large">
                        <button class="btn btn-small btn-success">
                            <i class="icon-signal"></i>
                        </button>

                        <button class="btn btn-small btn-info">
                            <i class="icon-pencil"></i>
                        </button>

                        <button class="btn btn-small btn-warning">
                            <i class="icon-group"></i>
                        </button>

                        <button class="btn btn-small btn-danger">
                            <i class="icon-cogs"></i>
                        </button>
                    </div>

                    <div class="sidebar-shortcuts-mini" id="sidebar-shortcuts-mini">
                        <span class="btn btn-success"></span>

                        <span class="btn btn-info"></span>

                        <span class="btn btn-warning"></span>

                        <span class="btn btn-danger"></span>
                    </div>
                </div>
                <!--#sidebar-shortcuts-->
                <!--  菜单-->
                <ul class="nav nav-list">

                    
                </ul>
                <!--/.nav-list-->

                <div class="sidebar-collapse" id="sidebar-collapse">
                    <i class="icon-double-angle-left" data-icon1="icon-double-angle-left" data-icon2="icon-double-angle-right"></i>
                </div>


                <script type="text/javascript">
                    try { ace.settings.check('sidebar', 'collapsed') } catch (e) { }
                </script>

            </div>
            <div class="main-content">
                <!-- 页面内容 -->
                @RenderBody()

                <!-- 右侧设置按钮 -->
                <div class="ace-settings-box" id="ace-settings-box">
                    <div>
                        <div class="pull-left">
                            <select id="skin-colorpicker" class="hide">
                                <option data-skin="default" value="#438EB9">#438EB9</option>
                                <option data-skin="skin-1" value="#222A2D">#222A2D</option>
                                <option data-skin="skin-2" value="#C6487E">#C6487E</option>
                                <option data-skin="skin-3" value="#D0D0D0">#D0D0D0</option>
                            </select>
                        </div>
                        <span>&nbsp; Choose Skin</span>
                    </div>

                    <div>
                        <input type="checkbox" class="ace ace-checkbox-2" id="ace-settings-navbar" />
                        <label class="lbl" for="ace-settings-navbar">Fixed Navbar</label>
                    </div>

                    <div>
                        <input type="checkbox" class="ace ace-checkbox-2" id="ace-settings-sidebar" />
                        <label class="lbl" for="ace-settings-sidebar">Fixed Sidebar</label>
                    </div>

                    <div>
                        <input type="checkbox" class="ace ace-checkbox-2" id="ace-settings-breadcrumbs" />
                        <label class="lbl" for="ace-settings-breadcrumbs">Fixed Breadcrumbs</label>
                    </div>

                    <div>
                        <input type="checkbox" class="ace ace-checkbox-2" id="ace-settings-rtl" />
                        <label class="lbl" for="ace-settings-rtl">Right To Left (rtl)</label>
                    </div>
                </div>
                <!--/#ace-settings-container-->
            </div>

        </div>
        <!--/.main-content-->
    </div>
    <!--/.main-container-->
    <!-- scroll up-->
    <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-small btn-inverse">
        <i class="icon-double-angle-up icon-only bigger-110"></i>
    </a>

    <!-- 底部 -->
    @*    <div id="div_foot" style="background-color: #F5F5F5; height: 35px; line-height: 35px; border-top: 1px solid #ccc; bottom: 0; position: absolute; width: 100%; text-align: center; margin: 11px 0 0 0;">
            技术支持：上海顺凯信息技术有限公司
        </div>*@

    <!--替代alert-->
    <div id="dialog-alert" class="hide">
        <div class="alert alert-info bigger-110">
            <span id="spanAlert"></span>
        </div>
    </div>

    <script type="text/javascript">
        if ("ontouchend" in document) document.write("<script src='@Url.Content("~/assets/js/jquery.mobile.custom.min.js")'>" + "<" + "/script>");
    </script>
    <script src="~/assets/js/bootstrap.min.js"></script>
    <script src="~/assets/js/bootbox.min.js"></script>

    <!--各类插件脚本-->
    @RenderSection("pluginJs", required: false)

    <!--ace scripts-->
    <script src="~/assets/js/ace-elements.min.js"></script>
    <script src="~/assets/js/ace.min.js"></script>
    <!--子界面自定义脚本-->
    @RenderSection("customJs", required: false)

    

</body>
</html>
